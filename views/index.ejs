  <% if (user) { %>
      <%- include('./partials/header') %>
    <div class = "main-list">
      <h1 style="font-size:40px"> Hello, <%= user.name %>! </h1>
      <h2 style="font-size:20px"> Your health log:</h2>
      <table id="list">
        <thead>
          <tr>
            <th>Date</th>
            <th>BMI</th>
            <th>Mood</th>
            <th>Weight</th>
            <th>Exercise</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
        <% logs.forEach(function(l) { %>
          <tr>
            <td><%= l.created_at.toString().substring(0, 15) %></td>
            <td><%= l.BMI %></td>
            <td><%= l.mood %></td>
            <td><%= l.weight %></td>
            <td><%= l.exercise %></td>
            <td>
              <form action="/logs/<%= l._id %>?_method=DELETE"
                id="delete-form" method="POST">
                <button type="submit">X</button>
              </form>
            </td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </br>
        <button><a href=customers/bmi>UPDATE LOG</button>
  </div>
  <% } else { %>
    <%- include('./partials/login-page') %>
  <% } %>
  <%- include('./partials/footer') %>

